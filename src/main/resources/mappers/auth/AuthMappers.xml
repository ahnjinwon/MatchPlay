<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sports.match.auth.model.mapper.AuthMapper">
    <select id="selectMemberById">
        SELECT *
        FROM MEMBER
        WHERE mem_id = #{memId}
    </select>
</mapper>